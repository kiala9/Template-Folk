#! /usr/bin/vvp
:ivl_version "12.0 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision - 12;
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/system.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_sys.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_textio.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/va_math.vpi";
S_0x562aa22204c0 .scope module, "led_demo_tb" "led_demo_tb" 2 3;
 .timescale -9 -12;
P_0x562aa2220650 .param/l "CLK_PERIOD" 0 2 6, +C4<00000000000000000000000000010100>;
v0x562aa226e3b0_0 .var "clk_tb", 0 0;
v0x562aa226e480_0 .net "led_tb", 0 0, v0x562aa226e1d0_0;  1 drivers
v0x562aa226e550_0 .var "rst_n_tb", 0 0;
S_0x562aa22206f0 .scope module, "dut_inst" "led_demo" 2 33, 3 2 0, S_0x562aa22204c0;
 .timescale -9 -12;
    .port_info 0 /INPUT 1 "clk";
    .port_info 1 /INPUT 1 "rst_n";
    .port_info 2 /OUTPUT 1 "led";
v0x562aa2245430_0 .net "clk", 0 0, v0x562aa226e3b0_0;  1 drivers
v0x562aa2245530_0 .var "cnt", 3 0;
v0x562aa226e1d0_0 .var "led", 0 0;
v0x562aa226e270_0 .net "rst_n", 0 0, v0x562aa226e550_0;  1 drivers
E_0x562aa224a5d0/0 .event negedge, v0x562aa226e270_0;
E_0x562aa224a5d0/1 .event posedge, v0x562aa2245430_0;
E_0x562aa224a5d0 .event/or E_0x562aa224a5d0/0, E_0x562aa224a5d0/1;
    .scope S_0x562aa22206f0;
T_0 ;
    %wait E_0x562aa224a5d0;
    %load/vec4 v0x562aa226e270_0;
    %nor/r;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.0, 8;
    %pushi/vec4 0, 0, 4;
    %assign/vec4 v0x562aa2245530_0, 0;
    %jmp T_0.1;
T_0.0 ;
    %load/vec4 v0x562aa2245530_0;
    %cmpi/u 10, 0, 4;
    %flag_inv 5; GE is !LT
    %flag_mov 8, 5;
    %jmp/0 T_0.2, 8;
    %pushi/vec4 0, 0, 4;
    %jmp/1 T_0.3, 8;
T_0.2 ; End of true expr.
    %load/vec4 v0x562aa2245530_0;
    %addi 1, 0, 4;
    %jmp/0 T_0.3, 8;
 ; End of false expr.
    %blend;
T_0.3;
    %assign/vec4 v0x562aa2245530_0, 0;
T_0.1 ;
    %jmp T_0;
    .thread T_0;
    .scope S_0x562aa22206f0;
T_1 ;
    %wait E_0x562aa224a5d0;
    %load/vec4 v0x562aa226e270_0;
    %nor/r;
    %flag_set/vec4 8;
    %jmp/0xz  T_1.0, 8;
    %pushi/vec4 0, 0, 1;
    %assign/vec4 v0x562aa226e1d0_0, 0;
    %jmp T_1.1;
T_1.0 ;
    %load/vec4 v0x562aa2245530_0;
    %cmpi/e 10, 0, 4;
    %jmp/0xz  T_1.2, 4;
    %load/vec4 v0x562aa226e1d0_0;
    %inv;
    %assign/vec4 v0x562aa226e1d0_0, 0;
T_1.2 ;
T_1.1 ;
    %jmp T_1;
    .thread T_1;
    .scope S_0x562aa22204c0;
T_2 ;
    %vpi_call 2 15 "$dumpfile", "wave.vcd" {0 0 0};
    %vpi_call 2 16 "$dumpvars", 32'sb00000000000000000000000000000000, S_0x562aa22204c0 {0 0 0};
    %end;
    .thread T_2;
    .scope S_0x562aa22204c0;
T_3 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x562aa226e3b0_0, 0, 1;
T_3.0 ;
    %delay 10000, 0;
    %load/vec4 v0x562aa226e3b0_0;
    %inv;
    %store/vec4 v0x562aa226e3b0_0, 0, 1;
    %jmp T_3.0;
    %end;
    .thread T_3;
    .scope S_0x562aa22204c0;
T_4 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x562aa226e550_0, 0, 1;
    %delay 60000, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x562aa226e550_0, 0, 1;
    %delay 2000000, 0;
    %vpi_call 2 29 "$finish" {0 0 0};
    %end;
    .thread T_4;
# The file index is used to find the file name in the following table.
:file_names 4;
    "N/A";
    "<interactive>";
    "./tb/led_demo_tb.v";
    "./rtl/led_demo.v";
